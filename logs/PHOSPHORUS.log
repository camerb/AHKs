     function getUPSlot(slotNo){
        var slotValue = Cookie.get("UserPreferences",function(str){
        	if(str.indexOf("%7C")>0) {
        		str = str.split("%7C")[slotNo];  
        	} else {
          		str = str.split("|")[slotNo];
          	}
          	str = unescape(str);
			return str.replace(/\+/g," ");
          	
        });
        return slotValue;
      }
      
      /**
      	Function created to save a new value to userprefs
      **/
      function newSetUserPrefs(itemNum, item) {
      	// set array into slots
      	var slots = Cookie.get("UserPreferences",function(str){
        	if(str.indexOf("%7C")>0) {
        		return str.split("%7C");  
        	} else {
          		return str.split("|");
          	}
        });
        if(slots!=null) {
        	slots[itemNum] = item;
        } else {
        	slots = new Array(33);
        	slots[33] = item;
        }
        var uCookie = slots.join("|");
        YAHOO.util.Cookie.set('UserPreferences', uCookie, {
					path:'/',
					expires: new Date("January 1, 2025"),
					domain:'weather.com'
		});
        
      }
      
